<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox fx:id="profile_topHBox_ID" alignment="CENTER" prefHeight="120.0" prefWidth="1218.0" spacing="50.0" style="-fx-background-color: #959595;" stylesheets="@../styles.css" BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.homepage.Profile_TOP_TAB_Controller">
   <children>
      <Button fx:id="profile_myPostID" mnemonicParsing="false" onAction="#switchToMyPosts" prefHeight="70.0" prefWidth="210.0" text="My Posts" textFill="WHITE">
         <font>
            <Font name="Arial Bold" size="30.0" />
         </font>
      </Button>
      <Button fx:id="profile_likedPostID" mnemonicParsing="false" onAction="#switchToLikedPosts" prefHeight="70.0" prefWidth="254.0" text="Liked Posts" textFill="WHITE">
         <font>
            <Font name="Arial Bold" size="30.0" />
         </font>
      </Button>
      <Button fx:id="profile_editProfileID" mnemonicParsing="false" onAction="#switchToEditProfile" prefHeight="70.0" prefWidth="210.0" text="Edit Profile" textFill="WHITE">
         <font>
            <Font name="Arial Bold" size="30.0" />
         </font>
      </Button>
      <Button fx:id="profile_contactUsID" mnemonicParsing="false" onAction="#switchToContactUs" prefHeight="70.0" prefWidth="277.0" text="Contact Us" textFill="WHITE">
         <font>
            <Font name="Arial Bold" size="30.0" />
         </font>
      </Button>
   </children>
   <BorderPane.margin>
      <Insets top="30.0" />
   </BorderPane.margin>
</HBox>
